function mAP = AP(sorted_list,type)
    % airplanes cars fcaes motorbikes 
    AP = 0;
    s=0;
    if type == 'airplanes'
        min = 0;
        max = 51;
    end
    if type == 'cars'
        min = 50;
        max = 101;
    end
    if type == 'faces'
        min = 100;
        max = 151;
    end
    if type == 'motorbikes'
        min = 150;
        max = 2001;
    end
    
    for i = 1: length(sorted_list)
        if sorted_list(i) > min && sorted_list(i) < max
            AP = AP + 1 / i;
            s=s+1;
        end
    end
    mAP = AP / s;

end
