function H = HE(D,centers,vocabulary_size)
    H = ones(1,vocabulary_size);
    whos
    class(D)
    [a] = size(D)
    for i = 1: length(D)
        [~,k] = min(pdist2(single(D(:,i)'),single(centers),'euclidean'));
%         [~,k] = min(vl_alldist(D(i,:),centers));
        H(k)  = H(k) + 1;
    end
    H = H / norm(H);
end
